, dimension (:) :: coeffs real ( dp ), intent ( in ) :: alpha real ( dp ), intent ( in ), dimension (:) :: thom_f , thom_s , thom_w , thom_p , strch integer :: i , j integer :: jm4 , jm3 , jm2 , jm1 , jp1 , jp2 , jp3 , jp4 real ( dp ) :: c_m4 , c_m3 , c_m2 , c_m1 , c_j , c_p1 , c_p2 , c_p3 , c_p4 real ( dp ), dimension ( SZ ) :: ss c_m4 = coeffs ( 1 ); c_m3 = coeffs ( 2 ); c_m2 = coeffs ( 3 ); c_m1 = coeffs ( 4 ) c_j = coeffs ( 5 ) c_p1 = coeffs ( 6 ); c_p2 = coeffs ( 7 ); c_p3 = coeffs ( 8 ); c_p4 = coeffs ( 9 ) ! Forward pass do j = 1 , n jm4 = modulo ( j - 5 , n ) + 1 jm3 = modulo ( j - 4 , n ) + 1 jm2 = modulo ( j - 3 , n ) + 1 jm1 = modulo ( j - 2 , n ) + 1 jp1 = modulo ( j - n , n ) + 1 jp2 = modulo ( j - n + 1 , n ) + 1 jp3 = modulo ( j - n + 2 , n ) + 1 jp4 = modulo ( j - n + 3 , n ) + 1 !$omp simd do i = 1 , SZ du ( i , j ) = c_m4 * u ( i , jm4 ) + c_m3 * u ( i , jm3 ) & + c_m2 * u ( i , jm2 ) + c_m1 * u ( i , jm1 ) & + c_j * u ( i , j ) & + c_p1 * u ( i , jp1 ) + c_p2 * u ( i , jp2 ) & + c_p3 * u ( i , jp3 ) + c_p4 * u ( i , jp4 ) & - du ( i , jm1 ) * thom_s ( j ) end do !$omp end simd end do ! Backward pass !$omp simd do i = 1 , SZ du ( i , n ) = du ( i , n ) * thom_w ( n ) end do !$omp end simd do j = n - 1 , 1 , - 1 !$omp simd do i = 1 , SZ du ( i , j ) = ( du ( i , j ) - thom_f ( j ) * du ( i , j + 1 )) * thom_w ( j ) end do !$omp end simd end do ! Periodic final pass !$omp simd do i = 1 , SZ ss ( i ) = ( du ( i , 1 ) - alpha * du ( i , n )) & / ( 1.0_dp + thom_p ( 1 ) - alpha * thom_p ( n )) end do !$omp end simd do j = 1 , n !$omp simd do i = 1 , SZ du ( i , j ) = ( du ( i , j ) - ss ( i ) * thom_p ( j )) * strch ( j ) end do !$omp end simd end do end subroutine der_univ_thom_per end module m_omp_kernels_thom","tags":"","url":"sourcefile/thomas.f90~2.html"},{"title":"exec_thom.f90 – x3d2","text":"This file depends on sourcefile~~exec_thom.f90~~EfferentGraph sourcefile~exec_thom.f90 exec_thom.f90 sourcefile~common.f90 common.f90 sourcefile~exec_thom.f